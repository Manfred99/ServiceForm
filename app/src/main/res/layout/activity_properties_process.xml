<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".process.PropertiesProcess">

    <Button
        android:id="@+id/btn_memoriaReal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="118dp"
        android:layout_marginLeft="118dp"
        android:layout_marginTop="59dp"
        android:text="Memoria Real"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_memoriaVirtual"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="Memoria Virtual"
        app:layout_constraintStart_toStartOf="@+id/btn_memoriaReal"
        app:layout_constraintTop_toBottomOf="@+id/btn_memoriaReal" />

    <Button
        android:id="@+id/btn_cpu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="CPU"
        app:layout_constraintStart_toStartOf="@+id/btn_memoriaVirtual"
        app:layout_constraintTop_toBottomOf="@+id/btn_memoriaVirtual" />

    <Button
        android:id="@+id/btn_prioridad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="Prioridad"
        app:layout_constraintStart_toStartOf="@+id/btn_cpu"
        app:layout_constraintTop_toBottomOf="@+id/btn_cpu" />

    <Button
        android:id="@+id/btn_tiempo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="29dp"
        android:text="Tiiempo de ejecuciÃ³n"
        app:layout_constraintStart_toStartOf="@+id/btn_prioridad"
        app:layout_constraintTop_toBottomOf="@+id/btn_prioridad" />

    <Button
        android:id="@+id/btn_killProcess"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginBottom="5dp"
        android:text="Terminar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn_stopProcess"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="38dp"
        android:layout_marginLeft="38dp"
        android:layout_marginBottom="5dp"
        android:text="Detener"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btn_killProcess" />

    <Button
        android:id="@+id/btn_createChild"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="43dp"
        android:layout_marginLeft="43dp"
        android:layout_marginBottom="5dp"
        android:text="Crear Hijos"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btn_stopProcess" />
</android.support.constraint.ConstraintLayout>